<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <ul>
                <% for (let order of orders) { %>
                    <li><%= order._id %></li>
                    <ul>
                        <% for (let product of order.items) { %>
                            <li>
                                <%= product.product.title %>(<%= product.quantity %>)
                            </li>
                        <% } %>
                    </ul>
                <% } %>
            </ul>
        </main>
        <%- include('../includes/end.ejs') %>